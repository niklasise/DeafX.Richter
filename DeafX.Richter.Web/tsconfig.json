{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "target": "es5",
    "module": "es6",
    "jsx": "react",
    "strictNullChecks": "false.",
    "lib": [ "es5", "es2017", "dom" ]
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "bin"
  ]
}
